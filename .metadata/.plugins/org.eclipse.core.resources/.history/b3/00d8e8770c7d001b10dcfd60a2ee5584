package class10;

public class Homework {

	public static void main(String[] args) {
		
		
	String []cars = {"Mazda", "BMW"	, "Subaru", "Toyota", "Honda", "Audi"};
	
	for(int i =0; i<cars.length; i++) {
		System.out.println(cars[i]);
	}
		
		

	}

}


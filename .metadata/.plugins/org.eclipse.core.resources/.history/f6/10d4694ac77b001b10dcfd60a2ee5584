package class09;

public class Pattern {
	public static void main(String[] args) {
		
		//printing stars in one line
		
		for(int row=1; row<=4; row++) {
			//System.out.print(" * " );
			
			for(int column=1;column<=5; column++) {
				System.out.print(" * ");
				
			}
			System.out.println();
			
			}
			
			
		}
		
		
		
		
		

		
	}



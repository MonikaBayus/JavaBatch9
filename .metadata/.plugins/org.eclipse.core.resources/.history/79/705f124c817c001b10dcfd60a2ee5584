package class10;

public class ArrayIntro {

	public static void main(String[] args) {
		
		
		int num = 10;
		num =20;
		System.out.println(num);
		
		int [] array = new int[5];
		
		array[0]=10;
		array[1]=12;
		array[2]=15;
		array[3]=9;
		array[4]=13;
		
		System.out.println(array[2-4]);

	}

}

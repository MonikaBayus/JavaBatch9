package class011;

public class Task2 {

	public static void main(String[] args) {
		
		String [][]names={
							{"Mr.", "Mrs.","Ms.", "Miss"},
							{"Smith", "Jordan", "Jackson", "Obama"}
		};
		
		System.out.println(names[0][0] + names[1][0]);
		System.out.println(names[0][1] + names[1][1]);
		System.out.println(names[0][2] + names[1][2]);



	}

}
